{
  "name": "@wywyjp/wyside",
  "version": "1.5.11",
  "type": "module",
  "description": "Apps Script IDE framework",
  "bin": "dist/src/index.js",
  "files": [
    "deploy-ui.mjs",
    "setup-svelte.mjs",
    "fix-animations.mjs",
    "dist/src",
    "dist/.gitignore-target",
    ".claspignore",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc.json",
    "jest.config.json",
    "LICENSE",
    "license-config.json",
    "license-header.txt",
    ".prettierignore",
    ".prettierrc.json",
    "rollup.config.mjs",
    "tsconfig.json",
    "template",
    "template-ui",
    "mcp-server"
  ],
  "repository": "wywyjp/wyside",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rimraf build",
    "lint": "eslint --fix src/ test/",
    "lint:staged": "lint-staged",
    "build": "npm run clean && npm run lint && tsc && npm run copy-files && (cd mcp-server && npm install && npm run build)",
    "test": "vitest run",
    "copy-files": "ncp .gitignore dist/.gitignore-target",
    "prepare": "is-ci || husky install",
    "test:e2e": "bash scripts/e2e-test.sh",
    "test:e2e:init": "bash scripts/e2e-init.sh",
    "test:e2e:clean": "rimraf test-projects"
  },
  "keywords": [
    "typescript",
    "apps-script",
    "ide",
    "google"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": "wywyjp inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/google-apps-script": "^1.0.59",
    "@types/node": "^22.19.1",
    "@types/prompts": "^2.4.3",
    "@types/write-file-atomic": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.1",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "license-check-and-add": "^4.0.5",
    "ncp": "^2.0.0",
    "prettier": "^2.8.5",
    "rimraf": "^4.4.0",
    "rollup": "^3.20.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^11.1.0",
    "json5": "^2.2.3",
    "lint-staged": "^13.2.0",
    "meow": "^11.0.0",
    "prompts": "^2.4.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-prettier": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "write-file-atomic": "^5.0.0",
    "@modelcontextprotocol/sdk": "^1.0.1"
  },
  "peerDependencies": {
    "typescript": ">=4"
  }
}
