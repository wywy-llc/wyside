# MCP Server Test Configuration
# Copy this file to .env and fill in your values

# Enable test mode (set to 'true' to run tests instead of starting server)
TEST_MODE=false

# Test sync_local_secrets
TEST_PROJECT_ID=your-gcp-project-id
TEST_SPREADSHEET_ID=your-spreadsheet-id

# Test scaffold_feature
TEST_FEATURE_NAME=Todo
TEST_FEATURE_OPERATIONS=create,getAll,getById,update,delete

# Google Cloud credentials path (optional)
GOOGLE_APPLICATION_CREDENTIALS=./secrets/service-account.json
